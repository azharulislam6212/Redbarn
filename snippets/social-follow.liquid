          <mask id="mask0_3_901" style="mask-type:luminance" maskUnits="userSpaceOnUse" x="0" y="0" width="4" height="4">
          <path d="M0 3.99963V0.522949H3.30938V3.99963H0Z" fill="currentColor"/>
          </mask>
          <g mask="url(#mask0_3_901)">
          <path fill-rule="evenodd" clip-rule="evenodd" d="M1.65533 0.522949C0.738298 0.522949 -0.000244141 1.30241 -0.000244141 2.26101C-0.000244141 3.22073 0.738298 3.99963 1.65533 3.99963C2.56809 3.99963 3.3093 3.22073 3.3093 2.26101C3.3093 1.30241 2.56809 0.522949 1.65533 0.522949Z" fill="currentColor"/>
          </g>
          </g>
          <defs>
          <clipPath id="clip0_3_901">
          <rect width="14" height="14" fill="currentColor" transform="translate(0 0.5)"/>
          </clipPath>
          </defs>
          </svg>
          
      </a>
    </li>
    {% endif %}
    
     {% if settings.social_vimeo_link != blank %}
    <li>
      <a href="{{ settings.social_vimeo_link | escape }}" target="_blank" role="button" aria-label="{{ 'general.social.vimeo' | t }}">
        <span class="icon icon-vimeo text-center"></span>
      </a>
    </li>
    {% endif %}
    
    {% if settings.social_pinterest_link != blank %}
    <li>
      <a href="{{ settings.social_pinterest_link | escape }}" target="_blank" role="button" aria-label="{{ 'general.social.pinterest' | t }}">
        <span class="icon icon-pinterest text-center"></span>
      </a>
    </li>
    {% endif %}
    {% if settings.social_instagram_link != blank %}
    <li>
      <a href="{{ settings.social_instagram_link | escape }}" target="_blank" role="button" aria-label="{{ 'general.social.instagram' | t }}">
        <span class="icon icon-instagram text-center"></span>
      </a>
    </li>
    {% endif %}
    {% if settings.social_tumblr_link != blank %}
    <li>
      <a href="{{ settings.social_tumblr_link | escape }}" target="_blank" role="button" aria-label="{{ 'general.social.tumblr' | t }}">
        <span class="icon icon-tumblr text-center"></span>
      </a>
    </li>
    {% endif %}
   
    {% if settings.social_vimeo_link != blank %}
    <li>
      <a href="{{ settings.social_vimeo_link | escape }}" target="_blank" role="button" aria-label="{{ 'general.social.vimeo' | t }}">
        <span class="icon icon-vimeo text-center"></span>
      </a>
    </li>
    {% endif %}
  </ul>
</div>
{% endif %}