{% if settings.addthis != blank %}
<div class="addthis_horizontal_follow_toolbox"></div>
{% elsif settings.social_twitter_link != blank or settings.social_facebook_link != blank or settings.social_pinterest_link != blank or settings.social_instagram_link != blank or settings.social_tumblr_link != blank or settings.social_snapchat_link != blank or settings.social_youtube_link != blank or settings.social_vimeo_link != blank %}
<div class="socials-list{{ social-class }}">
  <ul>
    {% if settings.social_facebook_link != blank %}
    <li>
      <a href="{{ settings.social_facebook_link | escape }}" target="_blank" role="button" aria-label="{{ 'general.social.facebook' | t }}">
        <span class="icon icon-facebook text-center"></span>
      </a>
    </li>
    {% endif %}
    {% if settings.social_twitter_link != blank %}
    <li>
      <a href="{{ settings.social_twitter_link | escape }}" target="_blank" role="button" aria-label="{{ 'general.social.twitter' | t }}">
        <span class="icon icon-twitter text-center"></span>
      </a>
    </li>
    {% endif %}
     {% if settings.social_linkedin_link != blank %}
    <li>
      <a href="{{ settings.social_linkedin_link | escape }}" target="_blank" role="button" aria-label="{{ 'general.social.linkedin' | t }}">

        <svg width="20" height="20" viewBox="0 0 14 15" fill="none" xmlns="http://www.w3.org/2000/svg">
          <g clip-path="url(#clip0_3_901)">
          <path fill-rule="evenodd" clip-rule="evenodd" d="M0.226562 14.9665H3.08293V5.31934H0.226562V14.9665Z" fill="currentColor"/>
