.gallery-top {
  .swiper-pagination {

    top: 50%;
    bottom: auto;
    width: 100%;
    transform: translateY(-50%);
    display: flex;
    justify-content: space-between;

    @media screen and (max-width:767px) {
      display:none;
    }

    .swiper-button {
      font-size:30px;
      width: 30px;
      height: 30px;
      display: flex;
      align-items: center;
      justify-content: center;
      background:#000;
      opacity:0;
      pointer-events:all;
      transition:all 0.3s;

      &.swiper-button-next {
        right:-60px;
      }
      &.swiper-button-prev {
        left:-60px;
      }

      svg {
        width: 17px;
        height: 17px;

        path {
          fill:#fff;
        }
      }


    }
  }

  &:hover {
    .swiper-pagination {
      .swiper-button {
        &[aria-disabled="false"] {
          opacity:1;
          pointer-events:all;
          cursor:pointer;

          &.swiper-button-next {
            right:0;
          }
          &.swiper-button-prev {
            left:0;
          }
        }
      }
    }
  }
}

.swiper-slide.thumb-image-product-template {
  &:not(.swiper-slide-thumb-active) {
    img {
      opacity:0.6;
    }
  }
}

.thumbnails  {

  .thumbs {
    font-size:20px;
    top: 50%;
    bottom: auto;
    transform: translateY(-50%);
    position:absolute;
    width: 30px;
    height: 30px;
    display: flex;
    align-items: center;
    justify-content: center;
    background:#000;

    svg {
      width: 17px;
      height: 17px;

      path {
        fill:#fff;
      }
    }

    &[aria-disabled="false"] {
      opacity:1;
      pointer-events:all;
      cursor:pointer;
    }
    &[aria-disabled="true"] {
      opacity:0;
      pointer-events:none;
    }
    &.swiper-button-next {
      right:0;
      left:auto;
    }
    &.swiper-button-prev {
      left:0;
      right:auto;
    }

    @media screen and (min-width:768px) {
      display:none;
    }
  }
}


