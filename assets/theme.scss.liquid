.container {
  margin:0 auto;
  padding:0 10px;
  max-width:1200px;

  &.full {
    max-width:1600px;
  }

  &.grid {
    @media #{$mobile} {
      padding:0;
    }
  }

  &.no-pad {
    padding:0 20px;
    max-width:1180px;

    @media #{$container} {
	  padding:0;
      max-width:1220px;
    }

  }

  &.remove-pad {
    padding:0;
  }

}

.one {width:8.33%;}		/* 100px / 1200 */
.two {width:16.66%;}	/* 200px */
.two-half {
  width:20%;

  @media #{$tablet} {

    .purchase-section & {
      width:33.33%
    }

  }

}						/* 250px */
.three {width:25%;}		/* 300px */
.four {width:33.33%;}	/* 400px */
.five {width:41.66%;}	/* 500px */
.six {width:50%;}		/* 600px */
.seven {width:58.33%;}	/* 700px */
.eight {width:66.66%;}	/* 800px */
.nine {width:75%;}		/* 900px */
.ten {width:83.33%;}	/* 1000px */
.eleven {width:91.66%;}	/* 1100px */
.twelve {width:100%;}	/* 1200px */

.one,
.two,
.two-half,
.three,
.four,
.five,
.six,
.seven,
.eight,
.nine,
.ten,
.eleven,
.twelve {
  float:left;
  font-size:$small;
  padding:0 $master_spacing / 2;
  @include border-box();

  &.negative-left {
    padding-left:0;
  }

  &.negative-right {
    padding-right:0;
  }

  &.negative-left,
  &.negative-right {

    &.positive {

      @media #{$mobile} {
        padding-left:$master_spacing / 2;
        padding-right:$master_spacing / 2;
      }

    }

  }

  .table-center & {

    &.hidden {
      display:none;
    }

    &.top {
      vertical-align:top;
    }

    &.no-height {
      height:auto;
    }

    &.brands {
      height:auto;
    }

    @media #{$mobile} {
      height:auto;

      &.brands,
      &.orders {
        width:50%;
      }

      .collection-products & {
        height: auto;
      }

    }

  }

  .table-center.product-variants & {
    vertical-align:top;
  }

  &.swiper-product {
    padding:0;

    @media #{$mobile} {

      .product {
        width:50%;
      }

    }

  }

  &.grid {
    @media #{$mobile} {
      padding-left:0;
      padding-right:0;
    }
  }

  &.margin-bottom {
    margin-bottom:$master_spacing / 2;
  }

  &.margin-top {
    margin-top:$master_spacing / 2;
  }

  &.margin-top-bottom {
    margin-bottom:$master_spacing / 2;
    margin-top:$master_spacing / 2;
  }

  @media #{$tablet} {

    .collection-products &,
    .cart-table &{
      width:100%;
    }

  }

  @media #{$mobile} {


    &:not(.mobile) {
      width:100%;
    }

    .purchase-section & {
      width:100%;
    }

    footer &:not(.brands) {
      padding:0 $master_spacing * 3;
    }

  }

}

.hidden {
  display:none;
  @include transition();
}

.left {
  float:left;
}

.right {
  float:right;
}

.center {
  display:block;
  float:none;
  margin:0 auto;
  text-align: center;

  &.center-table {
    display:table;
  }

  &.swiper-wrapper {
    display:block;
    height:auto;
  }

  @media #{$tablet} {

    &.swiper-wrapper {
      @include flex;
      height:100%;
    }

    .swiper-slide {
      height:auto;
    }

  }

}

.flex-wrap {
  @include flex-wrap;

  @media #{$mobile} {
    -webkit-flex-wrap: nowrap;
    -ms-flex-wrap: nowrap;
    flex-wrap: nowrap;
  }

}

.slide-center {
  float:none;
  margin: 0 auto;
}

.table {
  display:table;
  height:100%;
  width:100%;

  &.left-width {
    width: auto;

    .mobile-only & {
      width: 100%;
    }

  }

  &.icon-text-table {
    font-size:0;
    width: auto;

    .full-width & {
      width:100%;
    }

    .icon-text {
      @include border-box();
      display:inline-block;
      margin-bottom:$master_spacing / 2;
      padding:0 $master_spacing * 0.75;
      width:auto;

      .img {
        padding-right:$master_spacing / 2;

        img {
          margin:0;
          max-height:30px;
          max-width:30px;
        }

      }

    }

    @media #{$mobile} {
      margin: 0 auto;
      text-align:center;
    }

  }

}

.table-center {
  font-size:0;
}

.table-cell {
  display:table-cell;
  height:100%;
  vertical-align: middle;

  > div {
    padding-top:$master_spacing;

    &.no-height.image {
      padding-top:0;

      img,
      .placeholder-svg,
      .lazypadding div {
        margin-bottom:0;
      }

    }

    &.no-pad {
      padding-top:0;
    }

