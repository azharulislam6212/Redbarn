//	DEFAULT STYLES

$master_spacing:20px;

@mixin float-left() {
  float:left;
  width:100%;
}

@mixin opacity-100() {
  opacity:1;
  filter:alpha(opacity=100);
}

@mixin opacity-75() {
  opacity:0.75;
  filter:alpha(opacity=75);
}

@mixin opacity-50() {
  opacity:0.50;
  filter:alpha(opacity=50);
}

@mixin opacity-25() {
  opacity:0.25;
  filter:alpha(opacity=25);
}

@mixin opacity-0() {
  opacity:0;
  filter:alpha(opacity=0);
}

@mixin border-radius() {
  -webkit-border-radius:0;
  -moz-border-radius:0;
  border-radius:0;
}

@mixin background-cover() {
  -webkit-background-size:cover !important;
  -moz-background-size:cover !important;
  -o-background-size:cover !important;
  background-size:cover !important;
}

@mixin background-contain() {
  -webkit-background-size:contain !important;
  -moz-background-size:contain !important;
  -o-background-size:contain !important;
  background-size:contain !important;
}

@mixin border-box() {
  -webkit-box-sizing:border-box;
  -khtml-box-sizing:border-box;
  -moz-box-sizing:border-box;
  -ms-box-sizing:border-box;
  -o-box-sizing:border-box;
  box-sizing:border-box;
}

@mixin flex() {
  display: -webkit-box;
  display: -webkit-flex;
  display: -moz-box;
  display: -ms-flexbox;
  display: flex;
}

@mixin flex-wrap() {
  -webkit-flex-wrap: wrap;
  -ms-flex-wrap: wrap;
  flex-wrap: wrap;
}

@mixin inline-flex() {
  display: -webkit-inline-box;
  display: -webkit-inline-flex;
  display: -moz-inline-box;
  display: -ms-inline-flexbox;
  display: inline-flex;
}

@mixin flex-grow() {
  -webkit-box-flex: 1;
  -moz-box-flex: 1;
  -webkit-flex-grow: 1;
  -ms-flex: 1 1 auto;
  flex-grow: 1;
}

@mixin flex-direction() {
  -webkit-flex-direction: column;
  -ms-flex-direction: column;
  flex-direction: column;
}

@mixin loader() {
  animation:fancybox-rotate 1s linear infinite;
  background:transparent;
  border:2px solid rgba($button_color,0.25);
  border-bottom-color:$button_color;
  border-radius:50%;
  @include border-box();
  height:$master_spacing * 2.5;
  left:50%;
  margin-left:-$master_spacing * 1.25;
  margin-top:-$master_spacing * 1.25;
  opacity:.7;
  padding:0;
  position:absolute;
  top:50%;
  width:$master_spacing * 2.5;
  z-index:999;
}

@mixin transition() {
  -webkit-transition:all 0.25s ease 0s;
  -moz-transition:all 0.25s ease 0s;
  -ms-transition:all 0.25s ease 0s;
  -o-transition:all 0.25s ease 0s;
  transition:all 0.25s ease 0s;
  -webkit-backface-visibility: hidden;
  -moz-backface-visibility: hidden;
  -ms-backface-visibility: hidden;
  backface-visibility: hidden;
}

@mixin fast-transition() {
  -webkit-transition:all 0.125s ease 0s;
  -moz-transition:all 0.125s ease 0s;
  -ms-transition:all 0.125s ease 0s;
  -o-transition:all 0.125s ease 0s;
  transition:all 0.125s ease 0s;
  -webkit-backface-visibility: hidden;
  -moz-backface-visibility: hidden;
  -ms-backface-visibility: hidden;
  backface-visibility: hidden;
}

@mixin scroll-bar() {

  ::-webkit-scrollbar {
    -webkit-appearance: none;
    width: 14px;
  }

  ::-webkit-scrollbar-thumb {
    border:2px solid $white;
    background-color: $line_color;
  }

}


