                &:hover {
                  color:$button_color;
                  text-decoration:underline;
                }

              }

            }

          }

          &.hover,
          &.focused {

            li.hover li a {
              padding:$master_spacing / 2 0;
            }

            > a {
              color:darken($button_color, 15%);
            }

            > ul {
              border:none;
              position:relative;
              left:0;
            }

            a.active {
              color:$button_color;
            }

          }

          &.has-dropdown {

            > a {

              &:after {
                height:$master_spacing;
                line-height:$master_spacing;
                position: absolute;
                right: 0;
                text-align: center;
                top: 50%;
                -webkit-transform:translate(0,-50%);
                -moz-transform: translate(0,-50%);
                -o-transform: translate(0,-50%);
                -ms-transform:translate(0,-50%);
                transform:translate(0,-50%);
                width:$master_spacing;
              }

            }

          }

        }

      }

      @media #{$mobile} {
        display:none;
      }

    }

  }

