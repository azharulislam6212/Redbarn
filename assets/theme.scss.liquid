.tooltip {
  position:relative;
}

.tooltip-container {
  display:block;
  position:absolute;
  top:-100%;
  right:50%;
  z-index:3;
  color:$white;
  text-align:center;
  white-space:nowrap;
  -webkit-animation:popup 0.5s ease-in-out both 0.7s;
