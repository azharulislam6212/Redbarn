    a {
      padding:4px 0;

      &.hover {
        color:$button_color;
      }

    }

    ul.unformatted {
      padding:0;
    }

    li {
      display:block;
      font-size:$xx-small;
      font-weight:$body_font_weight_medium;
      line-height:$base * 1.5;
      padding:0;

      &:before {
        content:none;
      }

      a,
      span {
        display:inline-block;
        font-size:$xx-small;
        font-weight:$body_font_weight_medium;
        line-height:$base * 1.5;
        position:relative;

      }

      &.open-more-container a {
        font-weight:$body_font_weight_bold;
      }

      a:hover,
      a:focus,
      &.active a {
        text-decoration:none;
        @include opacity-100();
      }

    }

    &.formatted {

      li:not(.open-more-container) {

        a,
        &.disabled span {
          padding-left:$x-small * 2;
        }

        :before,
        :after {
          content:'';
          font-family: $icon_font;
          font-size:$small;
          left:0;
          @include opacity-50();
          position:absolute;
          text-align:center;
          top:50%;
          -webkit-transform: translate(0,-55%);
          -moz-transform: translate(0,-55%);
          -o-transform: translate(0,-55%);
          -ms-transform: translate(0,-55%);
          transform: translate(0,-55%);
        }

        a:before,
        &.disabled span:before {
          content:$box;
        }

        a.hover:after {
          color:$button_color;
          content:$square-solid;
          @include opacity-100();
          text-decoration:none;
        }

        a.hover:before,
        &.active a.hover:before {
          color:$body_font_color;
        }

        &.active a:after {
          color:$button_color;
          content:$square-solid;
          @include opacity-100();
          text-decoration:none;
        }

        &.active a.hover:after,
        &.active a:hover:after,
        &.active a:focus:after {
          content:$x-only;
        }

        &.disabled span {
          @include opacity-50();
          padding-bottom:4px;
          padding-top:4px;

          &:after {
            content:$square-crossed;
          }

        }

      }

    }

    @media #{$mobile} {
      margin:0 0 $master_spacing / 2;
      padding:0 $master_spacing * 3 $small / 2;

      .dropdown {
        margin-bottom:$master_spacing / 2;
      }

    }

  }

