          &.mega {
            @include float-left();

            > .three {
              display:none;
            }

            span.eight {
              float:none;
              padding:0;
            }

          }

        }

        > li {

/*           > a {
            @extend h5;
          }
 */
        }

        li {
          @include border-box();
          @include float-left();
          height: auto;
          padding-bottom:0;
          padding-top:0;
          text-align:left;

          a {
            display:block;
            margin:0;
            padding:0;
            position: relative;
            text-align: left;

            span {
              display:block;
              margin-right:$master_spacing * 1.5;
              padding: $master_spacing / 2 0;
